{
  "name": "research_vacant",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "rmdir dist /s /q & yarn build:frontend & yarn build:backend",
    "build:frontend": "yarn workspace @projects/frontend build",
    "build:backend": "yarn workspace @projects/backend build",
    "push": "yarn build & yarn clasp:push",
    "deploy": "yarn build & yarn clasp:push & yarn clasp:deploy",
    "clasp:push": "clasp push -f",
    "clasp:deploy": "clasp deploy -i AKfycbzEZbzutLFBXC9D_mSnhJExtIcvEsSF97j_PRip0nlwXIlKZt-fDb13TW23wC0C266a",
    "dev": "yarn workspace @projects/frontend dev",
    "open": "clasp open --webapp",
    "lint": "yarn lint:frontend"
  }
}
